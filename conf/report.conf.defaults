[Fingerbank::Unknown Fingerprints::Active]
type=sql
description=All active unknown Fingerbank fingerprints
columns=mac,dhcp_fingerprint,computername,user_agent,vendor
sql= <<EOT
BEGIN NOT ATOMIC
  SET @TENANT_ID = ?, @CURSOR = ?;
  SELECT
    node.mac,
    node.mac as vendor,
    dhcp_fingerprint,
    computername,
    user_agent
  FROM node, ip4log
  WHERE node.tenant_id = @TENANT_ID
    AND device_type IS NULL
    AND dhcp_fingerprint != 0
    AND dhcp_fingerprint >= @CURSOR
    AND ip4log.mac = node.mac
    AND ip4log.tenant_id = node.tenant_id
    AND (ip4log.end_time = 0 OR ip4log.end_time > NOW())
  ORDER BY mac
  LIMIT ?;
END;
EOT
