[storage configfiles]
driver=File
root_dir=/usr/local/pf/var/cache

[storage configfiles l1_cache]
storage=memcached

[storage configfilesdata]
driver=File
root_dir=/usr/local/pf/var/cache

[storage configfilesdata l1_cache]
storage=memcached

[storage httpd.admin]
driver=File
root_dir=/usr/local/pf/var/cache

[storage httpd.admin l1_cache]
storage=memcached

[storage httpd.portal]
driver=File
root_dir=/usr/local/pf/var/cache

[storage httpd.portal l1_cache]
storage=memcached

[storage pfdns]
driver=File
root_dir=/usr/local/pf/var/cache

[storage pfdns l1_cache]
storage=memcached

[storage memcached]
driver=Memcached
servers=127.0.0.1:11211
global=1
compress_threshold=10000

[storage file]
driver=File
root_dir=/usr/local/pf/var/cache

[dbi]
db=pf
user=pf
pass=packet
port=3306
host=localhost
