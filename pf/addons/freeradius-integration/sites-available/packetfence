server packetfence {

authorize {
        suffix
        preprocess
        eap {
                ok = return
        }
        files
        expiration
        logintime
	perl
}

authenticate {
        Auth-Type MS-CHAP {
                mschap
        }
        eap
}

preacct {
        preprocess
        acct_unique
        suffix
        files
}

accounting {
        detail
        unix
        radutmp
        attr_filter.accounting_response
	sql
}

session {
        radutmp
}

post-auth {
        exec
        perl
        Post-Auth-Type REJECT {
                attr_filter.access_reject
        }
}

pre-proxy {
}

post-proxy {
        eap
}
}
